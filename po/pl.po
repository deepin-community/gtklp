#: gtklpq/printer.c:721
msgid "."
msgstr " "

#: gtklpq/printer.c:818
msgid "No jobs."
msgstr "Brak zadań."

#: gtklpq/printer.c:852 gtklpq/printer.c:855 gtklpq/printer.c:859
msgid "Are you sure?"
msgstr "Czy jesteś pewien?"

#: gtklpq/printer.c:910
msgid "And for what reason ?"
msgstr "Z jakiego powodu?"

#: gtklpq/printer.c:943
msgid "Yes"
msgstr "Tak"

#: gtklpq/printer.c:947
msgid "No"
msgstr "Nie"

#: gtklpq/printer.c:960
msgid "Do you really want to cancel this job?"
msgstr "Czy na pewno chcesz anulować to zadanie?"

#: gtklpq/printer.c:968
msgid "Do you really want to cancel ALL jobs?"
msgstr "Czy na pewno chcesz anulować WSZYSTKIE zadania?"

#: gtklpq/printer.c:978
msgid "Do you really want to hold this job?"
msgstr "Czy na pewno chcesz wstrzymać to zadanie?"

#: gtklpq/printer.c:986
msgid "Do you really want to release this job?"
msgstr "Czy na pewno chcesz wznowić to zadanie?"

#: gtklpq/printer.c:998
msgid "Do you really want to move this Job to the following queue?"
msgstr "Czy na pewno chcesz przesunąć to zadanie do tej kolejki?"

#: gtklpq/printer.c:1008
msgid "Do you really want to stop this printer?"
msgstr "Czy na pewno chcesz zatrzymać drukarkę?"

#: gtklpq/printer.c:1016
msgid "Do you really want to start this printer?"
msgstr "Czy na pewno chcesz uruchomić drukarkę?"

#: gtklpq/printer.c:1026
msgid "Do you really want to change the job priority?"
msgstr "Czy na pewno chcesz zmienić priorytet zadania?"

#: gtklpq/printer.c:1037
msgid "Do you really want to reject jobs for this printer?"
msgstr "Czy na pewno chcesz odrzucić zadania drukowania dla tej drukarki?"

#: gtklpq/printer.c:1045
msgid "Do you really want to accept jobs for this printer?"
msgstr "Czy na pewno chcesz zaakceptować zadania dla tej drukarki?"

#: gtklpq/printer.c:1122 gtklpq/printer.c:1132
msgid "Unable to change priority!"
msgstr "Nie można zmienić priorytetu zadania!"

#: gtklpq/printer.c:1313
msgid "Job or printer not found!"
msgstr "Zadanie lub drukarka nie zostały znalezione!"

#: gtklpq/printer.c:1316
msgid "Authorization failed!"
msgstr "Autoryzacja nie powiodła się!"

#: gtklpq/printer.c:1319
msgid "You don't own this job!"
msgstr "Nie jesteś właścicielem tego zadania!"

#: gtklpq/printer.c:1323 gtklpq/printer.c:1346
msgid "Unable to do so!"
msgstr "Nie można tego zrobić!"

#: gtklpq/printer.c:1332
msgid "Unable to cancel job(s)!"
msgstr "Nie można anulować zadania!"

#: gtklpq/printer.c:1335
msgid "Unable to cancel printer!"
msgstr "Nie można przerwać drukowania!"

#: gtklpq/printer.c:1338
msgid "Unable to start printer!"
msgstr "Nie można uruchomić drukarki!"

#: gtklpq/printer.c:1341
msgid "Unable to accept jobs for this printer!"
msgstr "Nie można zaakceptować zadań dla tej drukarki!"

#: gtklpq/printer.c:1344
msgid "Unable to reject jobs for this printer!"
msgstr "Nie można odrzucić zadań dla tej drukarki!"

